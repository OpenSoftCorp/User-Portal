            $("#txtAutoSearch").autocomplete({
                source: function (request, response) {

                    $.ajax({
                        type: "GET",
                        url: "/Menu/GetSearchBox",
                        data: { ScreenName: request.term },
                        success: function (data) {


                            response($.map(data, function (item) {

                                return { lable: item, value: item }


                            }))
                        }
                    })
                }
            }).data("ui-autocomplete")._renderItem = function (ul, item) {

                var ScreenName = item.lable.split("-")[0];
                var Controller = item.lable.split("-")[1].split("/")[0].substring(1, item.lable.split("-")[1].split("/")[0].length);
                var Action = item.lable.split("-")[1].split("/")[1].substring(0, item.lable.split("-")[1].split("/")[1].length);
                var ScreenId = item.lable.split("-")[1].split("/")[2].substring(0, item.lable.split("-")[1].split("/")[2].length);
                var Address = "/" + Controller + "/" + Action + "/" + ScreenId;
                return $("<li></li>").data("item.autocomplete", item).append("<a href=" + Address + " style='color:#00acc1;font-family:Verdana;font-size:10px;font-weight:bold' target='_blank'>" + ScreenName + "</a>").appendTo(ul);
            };


            window.onscroll = function () {
                scrollFunction();
            };

            function scrollFunction() {

                if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
                    document.getElementById("myBtn").style.display = "block";
                }
                else {
                    document.getElementById("myBtn").style.display = "none";
                }

            }

            //// When the user clicks on the button, scroll to the top of the document

            function topFunction() {

                var doc = document.documentElement;
                var top = (window.pageYOffset || doc.scrollTop) - (doc.clientTop || 0);

                if (top > 0) {
                    document.body.scrollTop = 0;
                    document.documentElement.scrollTop = 0;
                }
                return false;
            }


         
        </script>

